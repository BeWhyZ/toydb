# Tests UPDATE index writes.

> CREATE TABLE "index" ( \
    id INT PRIMARY KEY, \
    "bool" BOOL INDEX, \
    "int" INT INDEX, \
    "float" FLOAT INDEX, \
    "string" STRING INDEX \
)
> INSERT INTO "index" VALUES (1, TRUE, 7, 3.14, 'foo')
---
ok

# An UPDATE writes to all indexes.
[ops]> UPDATE "index" SET "bool" = FALSE, "int" = 1, "float" = 2.718, "string" = 'bar'
