# A single-field update only updates the relevant index.
[ops]> UPDATE "index" SET "bool" = TRUE
---
storage set mvcc:NextVersion → 5 ["\x00" → "\x05"]
storage set mvcc:TxnActive(4) → "" ["\x01\x00\x00\x00\x00\x00\x00\x00\x04" → ""]
